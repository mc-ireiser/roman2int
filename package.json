{
  "name": "roman2int",
  "version": "1.0.9",
  "description": "Package to convert roman numerals to arabic number",
  "keywords": [
    "roman",
    "arabic",
    "numbers",
    "converter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mc-ireiser/roman2int"
  },
  "license": "MIT",
  "author": "Jesus Rodriguez",
  "main": "dist/index.min.js",
  "scripts": {
    "build": "npx babel src --out-dir lib",
    "format": "npx prettier --write src/index.js && npx sort-package-json",
    "format-lib": "npx prettier --write lib/index.js",
    "lint": "npx eslint src/index.js",
    "minify": "terser lib/index.js -o dist/index.min.js -c -m",
    "start": "npm-run-all lint format build format-lib test minify",
    "test": "jest test/roman2int.test.js",
    "test-minify": "jest test/min.test.js",
    "usage": "node usage/example.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "jest": "^25.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "sort-package-json": "^1.41.0",
    "terser": "^4.6.7",
    "typescript": "^3.8.3"
  }
}